{
  "name": "DARK_QUEEN",
  "description": "Sri Lankan Most Speed & Best Multi Device Whatsapp Bot",
  "logo": "https://files.catbox.moe/unnoua.jpg",
  "keywords": ["bot"],
  "success_url": "/",

  "env": {
    "SESSION_ID": {
      "description": "Put the session-id here.",
      "required": true,
      "default": ""
    },
    "PREFIX": {
      "description": "Add Your Custom Prefix here.",
      "required": true,
      "default": "."
    },
    "mode": {
      "description": "Add Your Bot Mode (private, public, inbox, groups) here.",
      "required": true,
      "default": "public"
    },
    "ANTI_BAD_WORDS_ENABLED": {
      "description": "Enable anti bad words (true/false)",
      "required": false,
      "default": "true"
    },
    "AUTO_READ_STATUS": {
      "description": "Auto read status (true/false)",
      "required": false,
      "default": "true"
    },
    "ANTI_BAD_WORDS": {
      "description": "Comma-separated list of bad words to filter.",
      "required": false,
      "default": ""
    },
    "ANTI_LINK": {
      "description": "Enable anti link feature (true/false)",
      "required": false,
      "default": "false"
    },
    "ALWAYS_ONLINE": {
      "description": "Always online feature (true/false)",
      "required": false,
      "default": "false"
    },
    "ALWAYS_TYPING": {
      "description": "Always typing feature (true/false)",
      "required": false,
      "default": "false"
    },
    "ALWAYS_RECORDING": {
      "description": "Always recording feature (true/false)",
      "required": false,
      "default": "false"
    },
    "ANTI_BOT": {
      "description": "Enable anti bot feature (true/false)",
      "required": false,
      "default": "true"
    },
    "ANTI_DELETE": {
      "description": "Enable anti delete feature (true/false)",
      "required": false,
      "default": "true"
    }
  },

  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  "stack": "heroku-24"
}
